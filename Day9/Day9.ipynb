    "import re\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "with open(\"inputPart1.txt\") as f:\n",
    "    ls = f.read().strip().split(\"\\n\")\n",
    "\n",
    "ns = np.array([list(map(int, re.findall(r\"-?\\d+\", x))) for x in ls])\n",
    "\n",
    "\n",
    "# Part 1+2\n",
    "for a in (ns, np.flip(ns, 1)):\n",
    "    print(np.sum([np.diff(a, k)[:,-1] for k in range(ns.shape[1])]))"
   ]
  }
